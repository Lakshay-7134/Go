    <script>



        function compactMode() {


            document.getElementById('compact').classList.add('active4');
            document.getElementById('comfort').classList.remove('active4');
            localStorage.setItem('WeUI-view-keycode', 'compact');


            document.getElementById("sidebar-container").classList.add("sidebar-collapsed");
            document.getElementById("uppertabbar").classList.add("tabbar-collapsed");
            var ib, tabcontentb;
            tabcontentb = document.getElementsByClassName("contentarea2");
            for (ib = 0; ib < tabcontentb.length; ib++) {
                tabcontentb[ib].classList.add("contentarea2-collapsed");
            }
        }
        function comfortMode() {
            document.getElementById('compact').classList.remove('active4');
            document.getElementById('comfort').classList.add('active4');
            localStorage.setItem('WeUI-view-keycode', 'comfort');

            document.getElementById("sidebar-container").classList.remove("sidebar-collapsed");
            document.getElementById("uppertabbar").classList.remove("tabbar-collapsed");
            var ib, tabcontentb;
            tabcontentb = document.getElementsByClassName("contentarea2");
            for (ib = 0; ib < tabcontentb.length; ib++) {
                tabcontentb[ib].classList.remove("contentarea2-collapsed");
            }

        }
        var viewcode = localStorage.getItem('WeUI-view-keycode');



        if (viewcode == 'compact') { compactMode(); }
        else { comfortMode(); }

        function checkpassword() {

            if (document.getElementById("securepassword-holder").value == '1331') { opensecure(); }

            else if (document.getElementById("securepassword-holder").value == '') { typepasswordfirst(); }
            else { alertsecure(); }
        }



        function opensecure() {
            document.getElementById("arrow-holder").classList.add("arrow-holder-2");
            document.getElementById("arrow-holder").classList.remove("arrow-holder-3");
            document.getElementById("pass-recognizer").innerHTML = "Password verified";
        }


        function typepasswordfirst() {


            document.getElementById("arrow-holder").classList.remove("arrow-holder-2");
            document.getElementById("pass-recognizer").innerHTML = "Type your password first";

        }

        function alertsecure() {

            document.getElementById("arrow-holder").classList.remove("arrow-holder-2");
            document.getElementById("arrow-holder").classList.add("arrow-holder-3");
            document.getElementById("pass-recognizer").innerHTML = "Invalid password";
        }

        function sidebar_toggle() {
            document.getElementById("mini-sidebar-container").classList.toggle("mini-sidebar-open");
            if (localStorage.getItem('WeUI-view-keycode') == 'compact') { comfortMode(); }

            else { compactMode(); }
        }

        function intfocus() {
            document.getElementById("search_container").classList.add("focused");
            document.getElementById("search_icon").classList.add("focu");
            document.getElementById('dropdowncontent2').classList.toggle('showit2');
            document.getElementById('notify-content').classList.remove('notify-showit');
            document.getElementById('myDropdown').classList.remove('showit');
            document.getElementById('progress-player-id').classList.remove('progress-player-show');
        }

        function intfocusrmv() {
            document.getElementById("search_container").classList.remove("focused");
            document.getElementById("search_icon").classList.remove("focu");
            document.getElementById("dropdowncontent2").classList.remove("showit2");
        }

        function opendropdown() {
            document.getElementById("myDropdown").classList.toggle("showit");
            document.getElementById('notify-content').classList.remove('notify-showit');
            document.getElementById('progress-player-id').classList.remove('progress-player-show');
        }

        var dark = document.querySelector("#dark-theme");
        var light = document.querySelector("#light-theme");


        function darkmode() {
            var element = document.body;
            element.classList.add("dark-mode");
            localStorage.setItem('WeUI-theme-keycode', 'dark');
        }
        function lightmode() {
            var element = document.body;
            element.classList.remove("dark-mode");
            localStorage.setItem('WeUI-theme-keycode', 'light');
        }

        dark.addEventListener('click', darkmode);
        light.addEventListener('click', lightmode);

        var themecode = localStorage.getItem('WeUI-theme-keycode');

        if (themecode == 'dark') { darkmode(); }
        else { lightmode(); }

        //     window.addEventListener('storage', () => {
        //         if (themecode.key == 'dark'){darkmode()}
        //       else if (themecode.key == 'light'){lightmode()}
        //       else{alert(localStorage.getItem('WeUI-theme-keycode').newValue)}
        //  })


        var varContainer = document.querySelector(":root");


        function changethemecolor(colorCode) {
            varContainer.style.setProperty('--themecolor', colorCode);
            // element.classList.add("dark-mode");
            localStorage.setItem('WeUI-theme-color-keycode', colorCode);

        }

        var themeColorCodeKey = localStorage.getItem('WeUI-theme-color-keycode');
        varContainer.style.setProperty('--themecolor', themeColorCodeKey);



        function open_notify() {
            document.getElementById('notify-content').classList.toggle('notify-showit');
            document.getElementById('myDropdown').classList.remove('showit');
            document.getElementById('progress-player-id').classList.remove('progress-player-show');
        }


        function open_progress_player() {
            document.getElementById('progress-player-id').classList.toggle('progress-player-show');
            document.getElementById('myDropdown').classList.remove('showit');
            document.getElementById('notify-content').classList.remove('notify-showit');
        }

        function open_nav() {
            document.body.classList.toggle('body-toggle');
            document.getElementById('line-1').classList.toggle('line-1-toggle');
            document.getElementById('line-2').classList.toggle('line-2-toggle');
            document.getElementById('line-3').classList.toggle('line-3-toggle');
            document.getElementById('menu-icon').classList.toggle('menu-icon-toggle');
        }

        function playaudio() {

            document.getElementById('play-id').classList.toggle('visible-1');
            document.getElementById('pause-id').classList.toggle('visible-2');
        }

        function openthemes() {
            document.getElementById("theme-container").classList.add("showit3");

        }
        function closethemes() {
            document.getElementById("theme-container").classList.remove("showit3");

        }
        function opensettings() {
            document.getElementById("settings-container").classList.add("showit3");

        }
        function closesettings() {
            document.getElementById("settings-container").classList.remove("showit3");

        }

        function showarrow() {


            let x = document.getElementById("uppertabbar").scrollLeft;
            if (x == 0) {
                document.getElementById("tabbar-shadow").style.display = "none";
            }

            else {
                document.getElementById("tabbar-shadow").style.display = "flex";

            }

        }

        function showarrowright() {


            var z = document.getElementById("uppertabbar").scrollLeft;
            var y = document.getElementById("uppertabbar").scrollWidth - document.getElementById("uppertabbar").clientWidth;


            if (z == y) {
                document.getElementById("tabbar-shadow-right").style.display = "none";
            }

            else {
                document.getElementById("tabbar-shadow-right").style.display = "flex";

            }

        }

        let slideIndex = 0;
        showSlides();

        function showSlides() {
            let i;
            let slides = document.getElementsByClassName("mySlides");
            let dots = document.getElementsByClassName("dot");
            for (i = 0; i < slides.length; i++) {
                slides[i].style.display = "none";
            }
            slideIndex++;
            if (slideIndex > slides.length) { slideIndex = 1 }
            for (i = 0; i < dots.length; i++) {
                dots[i].className = dots[i].className.replace(" active3", "");
            }
            slides[slideIndex - 1].style.display = "flex";
            dots[slideIndex - 1].className += " active3";
            setTimeout(showSlides, 5000); // Change image every 2 seconds
        }





        Amplitude.init({
            "bindings": {
                37: 'prev',
                39: 'next',
                32: 'play_pause'
            },

            "callbacks": {
                timeupdate: function () {
                    let percentage = Amplitude.getSongPlayedPercentage();

                    if (isNaN(percentage)) {
                        percentage = 0;
                    }

                    let slider = document.getElementById('song-percentage-played');
                    slider.style.width = percentage + '%';
                }
            },





            "songs": [
                {
                    "name": "Ep. 110",
                    "artist": "Toei Animation",
                    "album": "Son Goku Wakes! New Level of the Awakened!!",
                    "url": " https://drive.google.com/uc?export=download&id=1_C8qndpDYfD7HX6_73hK6LPVZ39hahR7",
                    "cover_art_url": "https://i1.sndcdn.com/artworks-000515401857-zj2fzl-t500x500.jpg"


                },

                {
                    "name": "1.2.3",
                    "artist": "The Pokemon Company",
                    "album": "Pokemon journeys",
                    "url": "https://drive.google.com/uc?export=download&id=1O68MfpzL5ugAcd20NnSMcRT4IWPTBvLl",
                    "cover_art_url": "https://github.com/Lakshay-7134/Go/blob/main/203bef4b39bd430556e4e764e6160f78.jpg?raw=true"
                }
            ]
        });

        window.onkeydown = function (e) {
            return !(e.keyCode == 32);
        };





        function openPage(pageName, elmnt) {
            var i, tabcontent, tablinks;
            tabcontent = document.getElementsByClassName("contentarea2");
            for (i = 0; i < tabcontent.length; i++) {
                tabcontent[i].style.display = "none";
            }
            tablinks = document.getElementsByClassName("tablink");
            for (i = 0; i < tablinks.length; i++) {
                tablinks[i].classList.remove("active");
            }

            document.getElementById(pageName).style.display = "initial";
            elmnt.classList.add("active");

        }


        document.getElementById("defaultOpen").click();



        function startTime() {
            const today = new Date();
            let h = today.getHours();
            let m = today.getMinutes();
            let s = today.getSeconds();
            m = checkTime(m);
            s = checkTime(s);
            document.getElementById('time').innerHTML = h + ":" + m + ":" + s;
            setTimeout(startTime, 1000);
        }

        function checkTime(i) {
            if (i < 10) { i = "0" + i };  // add zero in front of numbers < 10
            return i;
        }



    </script>
