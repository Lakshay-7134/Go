    <script>
        function collapsesidebar() {
            document.getElementById("menu-icon").click();
        }
        function intfocus() {
            document.getElementById("search_container").classList.add("focused");
            document.getElementById("search_icon").classList.add("focu");
            document.getElementById('dropdowncontent2').classList.toggle('showit2');
            document.getElementById('notify-content').classList.remove('notify-showit');
            document.getElementById('myDropdown').classList.remove('showit');
            document.getElementById('progress-player-id').classList.remove('progress-player-show');
        }

        function intfocusrmv() {
            document.getElementById("search_container").classList.remove("focused");
            document.getElementById("search_icon").classList.remove("focu");
            document.getElementById("dropdowncontent2").classList.remove("showit2");
        }

        function opendropdown() {
            document.getElementById("myDropdown").classList.toggle("showit");
            document.getElementById('notify-content').classList.remove('notify-showit');
            document.getElementById('progress-player-id').classList.remove('progress-player-show');
        }

        function darkmode() {
            var element = document.body;
            element.classList.toggle("dark-mode");
        }


        function open_notify() {
            document.getElementById('notify-content').classList.toggle('notify-showit');
            document.getElementById('myDropdown').classList.remove('showit');
            document.getElementById('progress-player-id').classList.remove('progress-player-show');
        }
        function open_progress_player() {
            document.getElementById('progress-player-id').classList.toggle('progress-player-show');
            document.getElementById('myDropdown').classList.remove('showit');
            document.getElementById('notify-content').classList.remove('notify-showit');
        }

        function open_nav() {
            document.body.classList.toggle('body-toggle');
            document.getElementById('line-1').classList.toggle('line-1-toggle');
            document.getElementById('line-2').classList.toggle('line-2-toggle');
            document.getElementById('line-3').classList.toggle('line-3-toggle');
            document.getElementById('menu-icon').classList.toggle('menu-icon-toggle');
        }
        function sidebar_toggle() {

            document.getElementById("sidebar-container").classList.toggle("sidebar-collapsed");
            document.getElementById("uppertabbar").classList.toggle("tabbar-collapsed");


        }
        function showarrow() {


            let x = document.getElementById("uppertabbar").scrollLeft;
            if (x != 0) {
                document.getElementById("tabbar-shadow").style.display = "flex";
            }

            else {
                document.getElementById("tabbar-shadow").style.display = "none";

            }

        }

        function showarrowright() {


            let z = document.getElementById("uppertabbar").scrollLeft;
            let y = document.getElementById("uppertabbar").width() - document.body.width();

            if (z = y) {
                document.getElementById("tabbar-shadow-right").style.display = "none";
            }

            else {
                document.getElementById("tabbar-shadow-right").style.display = "flex";

            }

        }


    </script>

</head>

<body onload="collapsesidebar()">

    <div class="flex_menu_container">



        <div class="flex_box_list_item" id="logo" style="justify-content: flex-start;cursor: default;">

